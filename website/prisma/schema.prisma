datasource db {
  provider             = "mysql"
  url                  = env("PLANETSCALE_DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model User {
  id            String  @id @default(uuid())
  email         String  @unique
  preferredName String?
  fullName      String
}
